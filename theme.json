    "styles": {
        "blocks": {
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/image": {
                "spacing": {
                    "margin": {
                        "top": "2.5rem",
                        "bottom": "2.5rem"
                    }
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "2.5"
                },
                "spacing": {
                    "margin": {
                        "top": "2rem",
                        "bottom": "2rem"
                    }
                }
            },
            "core/post-author": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/post-content": {
                "spacing": {
                    "margin": {
                        "bottom": "4rem"
                    }
                }
            },
            "core/post-featured-image": {
                "spacing": {
                    "margin": {
                        "top": "2rem",
                        "bottom": "2rem"
                    }
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--dark-2)",
            "text": "var(--wp--preset--color--light-2)"
        },
        "spacing": {
            "blockGap": "0"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "400"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-xl)",
                    "lineHeight": "1.3"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-xl)",
                    "lineHeight": "1.4"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-xl)"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)"
                },
                "spacing": {
                    "margin": {
                        "top": "3rem"
                    }
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--kide-purple)"
                },
                "typography": {
                    "textDecoration": "none"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "main",
            "title": "Main",
            "area": "uncategorized"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}