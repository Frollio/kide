    "styles": {
        "color": {
            "background": "var(--wp--preset--color--dark-8)",
            "text": "var(--wp--preset--color--light-2)"
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--20)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "400"
        },
        "blocks": {
            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    }
                }
            },
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/list": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "1.75"
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "1.75"
                }
            },
            "core/post-author": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/post-date": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                }
            },
            "core/post-terms": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                }
            },
            "core/post-title": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/query-title": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            }
        },
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--dark-10)"
                },
                ":hover": {
                    "color": {
                        "background": "var(--wp--preset--color--kide-purple)"
                    }
                },
                ":active": {
                    "color": {
                        "background": "var(--wp--preset--color--dark-10)"
                    }
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-xl)",
                    "lineHeight": "1.3"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-xl)",
                    "lineHeight": "1.4"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-xl)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--kide-purple)"
                },
                "typography": {
                    "textDecoration": "none"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
