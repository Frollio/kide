    "styles": {
        "blocks": {
            "core/columns": {
                "spacing": {
                    "margin": {
                        "bottom": "0"
                    }
                }
            },
            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    }
                }
            },
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--30)",
                        "bottom": "var(--wp--preset--spacing--20)"
                    }
                }
            },
            "core/image": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--20)",
                        "bottom": "var(--wp--preset--spacing--20)"
                    }
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "2.0"
                }
            },
            "core/post-author": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/post-date": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--10)",
                        "bottom": "var(--wp--preset--spacing--10)"
                    }
                }
            },
            "core/post-featured-image": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--20)",
                        "bottom": "var(--wp--preset--spacing--40)"
                    }
                }
            },
            "core/post-terms": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                },
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "0"
                    }
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--dark-7)",
            "text": "var(--wp--preset--color--light-2)"
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--20)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "400"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-xl)",
                    "lineHeight": "1.3"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-xl)",
                    "lineHeight": "1.4"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-xl)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--kide-purple)"
                },
                "typography": {
                    "textDecoration": "none"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}