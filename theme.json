        "spacing": {
            "blockGap": true,
            "margin": true,
            "padding": true,
            "units": [ "px", "em", "rem", "vh", "vw", "%" ],
            "spacingSizes": [
                {
                    "size": "clamp(0.5rem, calc(0.5rem + ((1vw - 0.25rem) * 0.5)), 1rem)",
                    "slug": "10",
                    "name": "Fluid 10"
                },
                {
                    "size": "clamp(1rem, calc(1rem + ((1vw - 0.25rem) * 1)), 2rem)",
                    "slug": "20",
                    "name": "Fluid 20"
                },
                {
                    "size": "clamp(1.5rem, calc(1.5rem + ((1vw - 0.25rem) * 1.5)), 3rem)",
                    "slug": "30",
                    "name": "Fluid 30"
                },
                {
                    "size": "clamp(2rem, calc(2rem + ((1vw - 0.25rem) * 2)), 4rem)",
                    "slug": "40",
                    "name": "Fluid 40"
                },
                {
                    "size": "clamp(2.5rem, calc(2.5rem + ((1vw - 0.25rem) * 2.5)), 5rem)",
                    "slug": "50",
                    "name": "Fluid 50"
                },
                {
                    "size": "clamp(3rem, calc(3rem + ((1vw - 0.25rem) * 3)), 6rem)",
                    "slug": "60",
                    "name": "Fluid 60"
                },
                {
                    "size": "clamp(3.5rem, calc(3.5rem + ((1vw - 0.25rem) * 3.5)), 7rem)",
