    "styles": {
        "blocks": {
            "core/image": {
                "spacing": {
                    "margin": {
                        "top": "2rem",
                        "bottom": "2rem"
                    }
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "2.5"
                },
                "spacing": {
                    "margin": {
                        "top": "2rem",
                        "bottom": "2rem"
                    }
                }
            },
            "core/post-author": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/post-featured-image": {
                "spacing": {
                    "margin": {
                        "top": "2rem",
                        "bottom": "2rem"
                    }
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--dark-2)",
            "text": "var(--wp--preset--color--light-2)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "400"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-xl)",
                    "lineHeight": "1.3"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-xl)",
                    "lineHeight": "1.4"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-xl)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                },
                "typography": {
                    "textDecoration": "none"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "main",
            "title": "Main",
            "area": "uncategorized"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}