{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "useRootPaddingAwareAlignments": true,
        "layout": {
            "contentSize": "1200px",
            "wideSize": "2000px"
        },
        "border": {
            "radius": true,
            "color": true,
            "style": true,
            "width": true
