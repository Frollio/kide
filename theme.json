    "styles": {
        "color": {
            "background": "var(--wp--preset--color--dark-4)",
            "text": "var(--wp--preset--color--light-1)"
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--20)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--poppins)",
            "fontSize": "16px",
            "fontStyle": "normal",
            "fontWeight": "300"
        },
        "blocks": {
            "core/button": {
                "color": {
                    "background": "var(--wp--preset--color--kide-dark-violet)"
                },
                "border": {
                    "color": "var(--wp--preset--color--kide-violet)",
                    "width": "2px"
                }
            },
            "core/calendar": {
                "color": {
                    "text": "var(--wp--preset--color--light-2)"
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                }
            },
            "core/code": {
                "border": {
                    "width": "0"
                },
                "color": {
                    "background": "var(--wp--preset--color--dark-10)"
                }
            },
            "core/cover":{
                "spacing": {
                    "padding": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    }
                },
                "dimensions": {
                    "minHeight": "0"
                }
            },
            "core/group": {
                "spacing": {
                    "padding": {
                        "top": "0",
                        "right": "0",
                        "bottom": "0",
                        "left": "0"
                    }
                }
            },
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                },
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--40)"
                    }
                }
            },
            "core/list": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "1.75"
                }
            },
            "core/navigation": {
                "typography": {
                    "fontWeight": "400"
                }
            },
            "core/paragraph": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)",
                    "lineHeight": "1.75"
                }
            },
            "core/post-author": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/post-date": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                }
            },
            "core/post-terms": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--s)"
                }
            },
            "core/post-title": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/query-title": {
                "color": {
                    "text": "var(--wp--preset--color--kide-violet)"
                }
            },
            "core/quote": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--s)"
                        },
                        "color": {
                            "text": "var(--wp--preset--color--kide-violet)"
                        }
                    }
                }
            },
            "core/search": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xs)"
                },
                "color": {
                    "background": "var(--wp--preset--color--dark-8)"
                }
            },
            "core/table": {
                "color": {
                    "background": "var(--wp--preset--color--dark-7)"
                }
            }
        },
        "elements": {
            "button": {
                "border": {
                    "style": "solid"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--4-xl)",
                    "lineHeight": "1.3"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--3-xl)",
                    "fontWeight": "600",
                    "lineHeight": "1.4"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--2-xl)",
                    "fontWeight": "600"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xl)",
                    "fontWeight": "600"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--l)",
                    "fontWeight": "500"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--m)",
                    "fontWeight": "400"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--kide-purple)"
                },
                "typography": {
                    "textDecoration": "none"
                },
                ":hover": {
                    "typography": {
                        "textDecoration": "underline"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
